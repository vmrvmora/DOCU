git init
git status
git add .
# eliminaci贸n de un archivo o carpeta del "stage"
git reset .DS_Store
git log
git config --global -e          ### edici贸n vi de las configuraciones globales
git config --global
git commit -m Primer commit

# Extraer el archivo del stage
git reset .DS_Store

git config --global init.defaultBranch main
git config --global user.mail victor.morales@vasscompany.com
# reponer el repositorio a como estaba en ultimo commit
git checkout -- .
git branch
git branch -m master main
git config --list
#Alias
git config --global alias.s status --short
git config --global alias.lg "log --graph --abbrev-commit --decorate --format=format:'%C(bold blue)%h%C(reset) - %C(bold green)(%ar)%C(reset) %C(white)%s%C(reset) %C(dim white)- %an%C(reset)%C(bold yellow)%d%C(reset)' --all"

# Eliminiaci贸n avisos de LF...
git config core.autocrlf true


# Edici贸n del 煤ltimo commit
git commit --amend

# stage y commit
git commit -am "Agregamos a Linterna Verde"

## Deshacer un commit para a帽adirle modificaciones a帽adidas de nuevo
